<script>
  function myrange(){
    var my = document.getElementById("myRange");
    var co = document.getElementById("coRange");

    var pr = document.getElementById("apiacc");
    var cou = document.getElementById("couRange");
    var lr = document.getElementById("layer4");
    var price = document.getElementById("show_price");
    var output = document.getElementById("sec");
    output.innerHTML = (my.value * 600);
    var hold=((((my.value*8)+(co.value*45)+(cou.value*150)+(pr.value*30)+lr.value*15))-28)
;
    price.innerHTML =""+hold+".00" ;
  }

  function corange(){
    var my = document.getElementById("myRange");
    var co = document.getElementById("coRange");

    var cou = document.getElementById("couRange");
    var lr = document.getElementById("layer4");
   var pr = document.getElementById("apiacc");
    var price = document.getElementById("show_price");
    var output = document.getElementById("con");
    output.innerHTML = co.value;
    var hold=((((my.value*8)+(co.value*45)+(cou.value*150)+(pr.value*30)+lr.value*15))-28);
    price.innerHTML =""+hold+".00" ;
     }

